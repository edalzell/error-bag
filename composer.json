{
    "name": "edalzell/error-bag",
    "license": "MIT",
    "description": "Display standard Laravel errors",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.61",
        "statamic/cms": "^3.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Edalzell\\ErrorBag\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Error Bag",
            "description": "Error Bag addon"
        },
        "laravel": {
            "providers": [
                "Edalzell\\ErrorBag\\ServiceProvider"
            ]
        }
    }
}
